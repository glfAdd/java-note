<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="glfadd.mapper.UserInfoMapper">
    <!-- 定义 SQL 映射关系 -->
    <select id="getUserById" parameterType="int" resultType="glfadd.dao.UserInfoPO">
        SELECT *
        from user_info
        WHERE id = #{id}
    </select>

</mapper>
